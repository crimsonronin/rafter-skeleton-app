{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "createClass": "createReactClass",
            "pragma": "React",
            "version": "15.0",
            "flowVersion": "0.53"
        },
        "propWrapperFunctions": [
            "forbidExtraProps"
        ]
    },
    "parser": "babel-eslint"
}